{
  "name": "hexo-github",
  "displayName": "hexo-github",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hexo-github.loginToGitHub",
        "title": "Login to GitHub"
      },
      {
        "command": "hexo-github.pullHexoRepo",
        "title": "Pull Hexo Repository"
      },
      {
        "command": "hexo-github.pushToHexoRepo",
        "title": "Push to Hexo Repository"
      },
      {
        "command": "hexo-github.server",
        "title": "server"
      },
      {
        "command": "hexo-github.new",
        "title": "new"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "hexo-github.pullHexoRepo",
          "group": "navigation"
        },
        {
          "command": "hexo-github.pushToHexoRepo",
          "group": "navigation"
        },
        {
          "command": "hexo-github.loginToGitHub",
          "group": "navigation"
        },
        {
          "command": "hexo-github.server",
          "group": "navigation"
        },
        {
          "command": "hexo-github.new",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.2",
    "axios": "^1.7.7",
    "express": "^4.21.1",
    "hexo": "^7.3.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^6.3.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0",
    "node-fetch": "^3.3.2",
    "open": "^10.1.0",
    "simple-git": "^3.27.0"
  }
}
